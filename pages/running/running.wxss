.tips{display: flex;flex-direction: row;justify-content: space-between;padding:0rpx 20rpx;box-sizing: border-box;width:100%;background:#fff;height:90rpx;line-height: 90rpx;}
.comp-name{font-size:28rpx;color: #101010;}
.status{font-size:28rpx;color:#15C179;}
.map{width:100%;background:#fff;padding:0 30rpx 20rpx;box-sizing: border-box;margin:10rpx 0;}
.map-nav{display: flex;flex-direction: row;height: 80rpx;line-height:80rpx;}
.listitem{color:#919191;width:120rpx;text-align:center;margin-right:30rpx;}
.active{color:#25AAFF;border-bottom: 2px solid #25AAFF;}
.current-state{font-size:28rpx;color: #101010;}
.currentj-title,.summary{margin:40rpx 0 20rpx;}
.current-desc{width:100%;background:#F7F8FB;border-radius: 6rpx;padding:30rpx 25rpx;box-sizing: border-box;display: flex;flex-direction: row;justify-content: space-between;}
.temperature{display: flex;flex-direction: column;align-items: center;}
.temperature text:first-child{margin-bottom: 20rpx;color:#919191;}
.temperature text:last-child{color: #101010;font-size: 32rpx;}
.summary text:first-child{color:#919191;font-size:28rpx;}
.record{width:100%;background:#fff;padding:0rpx 30rpx 20rpx;box-sizing: border-box;}
.record-title{height:60rpx;line-height: 60rpx;color: #919191;font-size:28rpx;}
.item{display: flex;flex-direction: row;justify-content: space-between;padding-right:100rpx;box-sizing: border-box;margin-top:20rpx;}
.btn{width:100%;margin:10rpx auto;background:#fff;height:120rpx;text-align:center;line-height: 120rpx;display: flex;align-items: center;justify-content: center;}
.btn .icon-fanhui{font-size:20rpx;margin-right:5rpx;}
.inner-btn{width:160rpx;color:#25AAFF;border:1px solid #25AAFF;height:60rpx;line-height: 60rpx;border-radius: 50rpx;}