  <view class="mypage">
    <view class="out-container">
      <view class="title">
        <text>烤房c</text>
        <text class="running"></text>
      </view>
      <view class="device-num">
        <text>设备型号：</text>
        <text>HBWH210532A01</text>
      </view>
      <view class="device-num">
        <text>管理员：</text>
        <text>小西</text>
        <view class="change">
          <text>更换</text>
          <text class="iconfont icon-jiantou"></text>
        </view>
      </view>
      <view class="device-name">
        <view class="itemlist">
          <text>设备名称</text>
          <text>尚未设置</text>
        </view>
        <view class="itemlist">
          <text>报警电话</text>
          <text>18699997777</text>
        </view>
      </view>
    </view>
    <view class="out-container">
      <!-- <view class="line-title">烘烤时长130-160个小时的拟合曲线</view> -->
      <view class="container-echart">
        <ec-canvas id="mychart-dom-bar" canvas-id="mychart-bar" ec="{{ ec }}"></ec-canvas>
      </view>
      <view class="xAxis">温度(℃)</view>
      <view class="yAxis">烘烤时间(小时)</view>
      <view class="default-param">
        <view class="list">
          <text>变黄期</text>
          <text>54小时</text>
        </view>
        <view class="list">
          <text>定色期</text>
          <text>55小时</text>
        </view>
        <view class="list">
          <text>香气</text>
          <text>10小时</text>
        </view>
        <view class="list">
          <text>干筋期</text>
          <text>49小时</text>
        </view>
      </view>
      <view class="default-param">
        <view class="timer">
          <text>烘烤时长</text>
          <text>100到130小时</text>
          <text>130到160小时</text>
          <text>160到190小时</text>
          <text>190小时以上</text>
        </view>
        <view class="timer">
          <text>拟合的烘烤炕数</text>
          <text>36</text>
          <text>296</text>
          <text>520</text>
          <text>380</text>
        </view>
        <view class="timer">
          <text>百分比</text>
          <text>2.9%</text>
          <text>23.7</text>
          <text>42.5</text>
          <text>30.9</text>
        </view>
      </view>
    </view>
    <view class="bake-btn">
      <text>历史烘烤</text>
      <text bindtap="handleTobaccoInfo">填写烟叶信息</text>
    </view>
  </view>